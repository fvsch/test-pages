{
  "headers": [
    {
      "source": "**/*.{svg,ico}",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/svg+xml"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "favicon.ico",
      "destination": "svg/screenshot.svg"
    }
  ],
  "directoryListing": false
}
